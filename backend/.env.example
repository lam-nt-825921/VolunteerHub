NODE_ENV=development
PORT=3000
# Database
DATABASE_URL="file:./dev.db"

# JWT Secrets - Replace with your own secure random strings (min 32 characters)
JWT_ACCESS_SECRET=your_very_very_long_random_string_here_2025_volunteerhub_access
JWT_REFRESH_SECRET=your_very_very_long_random_string_here_2025_volunteerhub_refresh
JWT_ACCESS_TOKEN_EXPIRES_IN=15m
JWT_REFRESH_TOKEN_EXPIRES_IN=7d

# Cloudinary - Replace with your own Cloudinary credentials
# Get these from: https://console.cloudinary.com/settings/api-keys
CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret

# Firebase - Replace with your own Firebase Admin SDK credentials
# Get these from: https://console.firebase.google.com/project/YOUR_PROJECT/settings/serviceaccounts/adminsdk
FIREBASE_PROJECT_ID=your-firebase-project-id
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYOUR_PRIVATE_KEY_HERE\n-----END PRIVATE KEY-----\n"
FIREBASE_CLIENT_EMAIL=firebase-adminsdk-xxxxx@your-project-id.iam.gserviceaccount.com

# Socket.IO CORS
SOCKET_CORS_ORIGIN=http://localhost:4200

# Invite Code Secret - Replace with your own secure random string (min 32 characters)
INVITE_CODE_SECRET=your_very_long_random_secret_for_invite_code_generation
